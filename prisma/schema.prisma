generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model user {
    id       BigInt @id @default(autoincrement()) @db.UnsignedBigInt
    email    String
    password String
    avatar   String
    github   String
    douyin   String
    weibo    String
    wakatime String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
